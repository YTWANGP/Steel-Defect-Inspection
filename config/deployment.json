        "modules": {
          "camera-capture": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "env": {
              "VIDEO_PATH": {
                "value": 0
              },
              "IMAGE_PROCESSING_ENDPOINT": {
                "value": "http://image-classifier-service:80/image"
              },
              "RESIZE_WIDTH": {
                "value": 256
              },
              "RESIZE_HEIGHT": {
                "value": 256
              },
              "SHOW_VIDEO": {
                "value": "True"
              }
            },
            "settings": {
              "image": "miabcontainerregistry.azurecr.io/cameracapture:latest-test-amd64",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5012/tcp\":[{\"HostPort\":\"5012\"}]},\"Binds\":[\"/dev/video0:/dev/video0\",\"/media:/media\"],\"Devices\":[{\"PathOnHost\":\"/dev/video0\",\"PathInContainer\":\"/dev/video0\",\"CgroupPermissions\":\"mrw\"}]}}"
            }
          },
          "image-classifier-service": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "miabcontainerregistry.azurecr.io/imageclassifierservice:latest-amd64",
              "createOptions": ""
            }
          },
          "blob-service": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "env": {
              "FILE_PATH": {
                "value": "/media"
              },
              "ACCOUNT_NAME": {
                "value": "miabstorageaccount"
              },
              "ACCOUNT_KEY": {
                "value": "lNIKNR1FEb/E3G+5LHxDrYx4mMoou+V4wDW18NP43XY29wiGifqPU2dpLqLFhbyayekq6k3FAdnf+v5yn2LIXQ=="
              }
            },
            "settings": {
              "image": "miabcontainerregistry.azurecr.io/blobservice:latest-test-amd64",
              "createOptions": "{\"HostConfig\":{\"Binds\":[\"/media:/media\"]}}"
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "CameraCaptureToIoTHub": "FROM /messages/modules/camera-capture/outputs/output1 INTO $upstream",
          "CameraCaptureToBlobService": "FROM /messages/modules/camera-capture/outputs/output1 INTO BrokeredEndpoint(\"/modules/blob-service/inputs/input1\")"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
